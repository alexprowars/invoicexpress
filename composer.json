{
    "name": "indigit/invoicexpress",
    "description": "A simple PHP wrapper for InvoiceXpress API",
    "homepage": "https://indigit.pt",
    "version": "1.1.1",
    "keywords": [
        "invoicexpress",
        "invoice",
        "api"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "InvoiceXpress\\": "src/InvoiceXpress/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
